namespace Reports
{
	public class Report : Stimulsoft.Report.StiReport
	{
		public Report()        {
			this.InitializeComponent();
		}

		#region StiReport Designer generated code - do not modify
		#endregion StiReport Designer generated code - do not modify
		
		public string DateToPersian(object date) 
		{ 
			System.Globalization.PersianCalendar persian = new System.Globalization.PersianCalendar(); 

			DateTime greg = (DateTime)date; 
			int year = persian.GetYear(greg); 
			int month = persian.GetMonth(greg); 
			int day = persian.GetDayOfMonth(greg); 

			return year.ToString()+"/"+month.ToString() + "/" + day.ToString();
		} 
	}
}
